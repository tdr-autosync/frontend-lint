{
  "name": "@motoinsight/lint",
  "version": "4.0.2",
  "description": "Motoinsight Frontend Linter",
  "author": "Motoinsight",
  "license": "UNLICENSED",
  "main": "index.js",
  "files": [
    "index.js",
    "cli.js",
    "prettier-config-default.js",
    "stylelint-config-default.js"
  ],
  "bin": {
    "motoinsight-lint": "./cli.js"
  },
  "dependencies": {
    "@motoinsight/eslint-plugin-motoinsight": "github:unhaggle/eslint-plugin-motoinsight#v4.0.0",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "chalk": "4.1.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-vue": "7.17.0",
    "globby": "11.0.4",
    "prettier": "2.3.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.20.1"
  },
  "devDependencies": {
    "typescript": "4.4.2"
  },
  "peerDependencies": {
    "typescript": "^4.4.2"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "ignore"
  }
}
